    {
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["react-app", "prettier",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "react/prop-types": 0,
        "prettier/prettier": ["error", {"endOfLine":"auto"}]
    }
}
